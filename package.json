{
  "name": "morteam-web",
  "version": "0.0.1",
  "description": "The new web client for MorTeam, the FRC collaboration tool",
  "scripts": {
    "build": "./node_modules/gulp/bin/gulp.js build",
    "watch": "./node_modules/gulp/bin/gulp.js watch",
    "vendor": "./node_modules/gulp/bin/gulp.js vendor",
    "postinstall": "npm run vendor",
    "fmt": "for f in `find src -name '*.js'`; do ./node_modules/js-beautify/js/bin/js-beautify.js --type js -r -n -X -e \"\\n\" -b collapse-preserve-inline $f; done | grep '\\.js$' || echo done"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mortorqrobotics/morteam-web.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mortorqrobotics/morteam-web/issues"
  },
  "homepage": "https://github.com/mortorqrobotics/morteam-web",
  "dependencies": {
    "axios": "^0.12.0",
    "babel-runtime": "^6.9.2",
    "radium": "^0.18.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-root-import": "^4.0.1",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "gulp": "^3.9.1",
    "gulp-streamify": "^1.0.2",
    "gulp-uglify": "^1.5.4",
    "js-beautify": "^1.6.3",
    "react-bootstrap": "^0.29.5",
    "vinyl-source-stream": "^1.1.0"
  }
}
